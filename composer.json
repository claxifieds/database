{
  "name": "claxifieds/database",
  "description": "Claxifieds database library",
  "license": "Apache-2.0",
  "keywords": [
    "database",
    "osclass"
  ],
  "type": "library",
  "require": {
    "php": ">=5.6.3",
    "ext-mysqli": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.2",
    "squizlabs/php_codesniffer": "1.5.3",
    "phpmd/phpmd": "@stable",
    "pdepend/pdepend": "2.4.0"
  },
  "autoload": {
    "psr-4":  {
      "Claxifieds\\Database\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
