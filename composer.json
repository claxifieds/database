{
  "name": "claxifieds/database",
  "description": "Claxifieds database library",
  "license": "Apache-2.0",
  "keywords": [
    "database",
    "osclass"
  ],
  "type": "library",
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:claxifieds/logger.git"
    }
  ],
  "require": {
    "php": ">=5.6.3",
    "ext-mysqli": "*",
    "claxifieds/logger": "master"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.2",
    "squizlabs/php_codesniffer": "1.5.3",
    "phpmd/phpmd": "@stable",
    "pdepend/pdepend": "2.4.0"
  },
  "autoload": {
    "psr-4":  {
      "Claxifieds\\Database\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
